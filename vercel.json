{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/admin",
      "destination": "https://REPLACE_WITH_YOUR_ADMIN_VERCEL_URL/admin"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://REPLACE_WITH_YOUR_ADMIN_VERCEL_URL/admin/:path*"
    }
  ]
}




